{
  admin off
}


example.com {
  @allowlist {
    remote_ip  8.8.8.8/32
  }

  @not_allowlist {
    not remote_ip  8.8.8.8/32
  }
  
  
  handle  {
    reverse_proxy @allowlist 192.168.50.2
    respond @not_allowlist 404
  }
  
  }


http://example.com:8080, http://example.com:1337 {
  redir http://example.com{uri} permanent
}

